{
    "name": "@goodbyenjn/utils",
    "version": "1.3.1",
    "description": "GoodbyeNJN's utils for typescript and javascript",
    "keywords": [
        "utils",
        "typescript",
        "javascript"
    ],
    "homepage": "https://github.com/GoodbyeNJN/utils",
    "bugs": {
        "url": "https://github.com/GoodbyeNJN/utils/issues"
    },
    "license": "MIT",
    "author": "GoodbyeNJN <cc@fuckwall.cc>",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/GoodbyeNJN/utils.git"
    },
    "type": "module",
    "exports": {
        ".": {
            "types": "./dist/common.d.ts",
            "import": "./dist/common.js"
        },
        "./fs": {
            "types": "./dist/fs.d.ts",
            "import": "./dist/fs.js"
        },
        "./remeda": {
            "types": "./dist/remeda.d.ts",
            "import": "./dist/remeda.js"
        },
        "./result": {
            "types": "./dist/result.d.ts",
            "import": "./dist/result.js"
        },
        "./types": {
            "types": "./dist/types.d.ts"
        },
        "./global-types": {
            "types": "./dist/global-types.d.ts"
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "dev": "pnpm run clean && pnpm run rolldown -w",
        "build": "pnpm run clean && pnpm run rolldown",
        "clean": "rm -rf dist",
        "rolldown": "rolldown -c rolldown.config.ts",
        "check": "tsc && eslint && prettier --check .",
        "type": "tsc",
        "lint": "eslint --fix",
        "format": "prettier --write .",
        "test": "vitest",
        "release": "bumpp"
    },
    "devDependencies": {
        "@goodbyenjn/configs": "^6.1.2",
        "@goodbyenjn/utils": "file:",
        "@types/node": "^24.7.2",
        "eslint": "^9.37.0",
        "prettier": "^3.6.2",
        "remeda": "^2.26.1",
        "rolldown": "1.0.0-beta.42",
        "rolldown-plugin-dts": "^0.16.11",
        "rotery": "^0.7.0",
        "safe-stable-stringify": "^2.5.0",
        "tinyglobby": "^0.2.15",
        "type-fest": "^4.41.0",
        "typescript": "^5.9.3",
        "vitest": "^3.2.4"
    },
    "publishConfig": {
        "registry": "https://registry.npmjs.org",
        "access": "public"
    },
    "engines": {
        "node": ">=20"
    }
}
